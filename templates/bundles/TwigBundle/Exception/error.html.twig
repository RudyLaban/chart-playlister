{# templates/bundles/TwigBundle/Exception/error404.html.twig #}
{% extends 'base.html.twig' %}

{% block body %}

    <section class="error pt-5 pb-4 text-center container-fluid">
        <div class="row py-3">
            <h2 class="pt-4">{{ head_title }}</h2>
            <div class="col-lg-6 col-8 mx-auto">
                <img src="{{ asset( image ) }}" alt="">
            </div>
            <p class="px-4 pt-3 pb-1">{{ text|raw }}</p>
        </div>
    </section>

{% endblock %}

{% block script %}
    <script src="{{ asset('js/custom.js') }}"></script>
{% endblock %}